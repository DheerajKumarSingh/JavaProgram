package linkedlist;

public class SinglyLinkedList {
	
	Node head;
	Node tail;

	public SinglyLinkedList() {
		head=null;
		tail=null;
	}
	
	
	
	void insertNode() {
		
		
		
	}
	
	Node createNode(int data) {
		Node node=new Node();
		node.data=data;
		node.next=null;
		return node;
	}
	
	
	 class Node{
		Node next;
		int data;
	}

}
